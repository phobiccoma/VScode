// Place your key bindings in this file to override the defaultsauto[]
[
	{
		"key": "ctrl+k",
		"command": "workbench.action.quickOpenView"
	},
	{
		"key": "ctrl+q",
		"command": "-workbench.action.quickOpenView"
	},
	{
		"key": "ctrl+alt+e",
		"command": "editor.foldAll",
		"when": "editorTextFocus && foldingEnabled"
	},
	{
		"key": "ctrl+k ctrl+0",
		"command": "-editor.foldAll",
		"when": "editorTextFocus && foldingEnabled"
	},
	{
		"key": "ctrl+numpad5",
		"command": "workbench.action.quickOpenNavigateNextInViewPicker",
		"when": "inQuickOpen && inViewsPicker"
	},
	{
		"key": "ctrl+q",
		"command": "-workbench.action.quickOpenNavigateNextInViewPicker",
		"when": "inQuickOpen && inViewsPicker"
	},
	{
		"key": "ctrl+e",
		"command": "-workbench.action.quickOpenNavigateNextInFilePicker",
		"when": "inFilesPicker && inQuickOpen"
	},
	{
		"key": "ctrl+e",
		"command": "-workbench.action.quickOpen"
	},
	{
		"key": "alt+e",
		"command": "editor.toggleFold",
		"when": "editorTextFocus && foldingEnabled"
	},
	{
		"key": "ctrl+k ctrl+l",
		"command": "-editor.toggleFold",
		"when": "editorTextFocus && foldingEnabled"
	},
	{
		"key": "alt+q",
		"command": "editor.action.commentLine",
		"when": "editorTextFocus && !editorReadonly"
	},
	{
		"key": "ctrl+/",
		"command": "-editor.action.commentLine",
		"when": "editorTextFocus && !editorReadonly"
	},
	{
		"key": "ctrl+alt+c",
		"command": "extension.ecsstractor_port_runwithbem"
	},
	{
		"key": "alt+f6",
		"command": "editor.action.showAccessibilityHelp"
	},
	{
		"key": "alt+f1",
		"command": "-editor.action.showAccessibilityHelp"
	},
	{
		"key": "ctrl+alt+f2",
		"command": "workbench.action.keybindingsReference"
	},
	{
		"key": "ctrl+k ctrl+r",
		"command": "-workbench.action.keybindingsReference"
	},
	{
		"key": "alt+f1",
		"command": "workbench.action.openGlobalKeybindings"
	},
	{
		"key": "ctrl+k ctrl+s",
		"command": "-workbench.action.openGlobalKeybindings"
	},
	{
		"key": "ctrl+shift+c",
		"command": "extension.ecsstractor_port_run"
	},
	{
		"key": "ctrl+shift+c",
		"command": "-workbench.action.terminal.openNativeConsole",
		"when": "!terminalFocus"
	},
	{
		"key": "ctrl+f1",
		"command": "workbench.action.showCommands"
	},
	{
		"key": "f1",
		"command": "-workbench.action.showCommands"
	},
	{
		"key": "f1",
		"command": "editor.action.revealDefinition",
		"when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
	},
	{
		"key": "f12",
		"command": "-editor.action.revealDefinition",
		"when": "editorHasDefinitionProvider && editorTextFocus && !isInEmbeddedEditor"
	},
	{
		"key": "ctrl+shift+t",
		"command": "translates.replace",
		"when": "editorHasSelection"
	},
	{
		"key": "ctrl+alt+t",
		"command": "-translates.replace",
		"when": "editorHasSelection"
	},
	{
		"key": "ctrl+shift+t",
		"command": "-translates.translates",
		"when": "editorHasSelection"
	},
	{
		"key": "alt+m",
		"command": "ysemeniuk.emmetLive",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+k ctrl+l",
		"command": "-ysemeniuk.emmetLive",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+b",
		"command": "-workbench.action.toggleSidebarVisibility"
	},
	{
		"key": "ctrl+b",
		"command": "workbench.action.showEmmetCommands"
	},
	{
		"key": "ctrl+numpad1",
		"command": "editor.emmet.action.wrapWithAbbreviation"
	},
	{
		"key": "alt+b",
		"command": "editor.action.insertSnippet"
	},
	{
		"key": "alt+numpad0",
		"command": "workbench.userDataSync.actions.resolveSnippetsConflicts"
	},
	{
		"key": "alt+f2",
		"command": "workbench.action.openSnippets"
	},
	{
		"key": "alt+x",
		"command": "editor.action.deleteLines",
		"when": "textInputFocus && !editorReadonly"
	},
	{
		"key": "ctrl+shift+k",
		"command": "-editor.action.deleteLines",
		"when": "textInputFocus && !editorReadonly"
	},
	{
		"key": "alt+d",
		"command": "deleteAllRight"
	},
	{
		"key": "shift+delete",
		"command": "-editor.action.clipboardCutAction"
	},
	{
		"key": "alt+numpad2",
		"command": "translates.switch"
	},
	{
		"key": "alt+numpad3",
		"command": "translates.settings"
	},
	{
		"key": "alt+numpad4",
		"command": "translates.hover",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+k alt+t",
		"command": "-translates.hover",
		"when": "editorTextFocus"
	},
	{
		"key": "ctrl+q",
		"command": "editor.action.transformToUppercase"
	},
	{
		"key": "ctrl+e",
		"command": "editor.action.transformToLowercase"
	},
	{
		"key": "alt+f",
		"command": "transliterate.toLatin"
	},
	{
		"key": "alt+g",
		"command": "workbench.action.splitEditor"
	},
	{
		"key": "ctrl+\\",
		"command": "-workbench.action.splitEditor"
	},
	{
		"key": "alt+r",
		"command": "-toggleSearchRegex",
		"when": "searchViewletFocus"
	},
	{
		"key": "alt+r",
		"command": "-toggleFindRegex",
		"when": "editorFocus"
	},
	{
		"key": "alt+r",
		"command": "-workbench.action.terminal.toggleFindRegex",
		"when": "terminalFindFocused && terminalProcessSupported || terminalFocus && terminalProcessSupported"
	},
	{
		"key": "alt+r",
		"command": "-toggleSearchEditorRegex",
		"when": "inSearchEditor && searchInputBoxFocus"
	},
	{
		"key": "ctrl+r",
		"command": "revealFileInOS",
		"when": "!editorFocus"
	},
	{
		"key": "shift+alt+r",
		"command": "-revealFileInOS",
		"when": "!editorFocus"
	},
	{
		"key": "alt+c",
		"command": "editor.action.copyLinesDownAction",
		"when": "editorTextFocus && !editorReadonly"
	},
	{
		"key": "shift+alt+down",
		"command": "-editor.action.copyLinesDownAction",
		"when": "editorTextFocus && !editorReadonly"
	},
	{
		"key": "ctrl+t",
		"command": "workbench.action.terminal.toggleTerminal",
		"when": "terminal.active"
	},
	{
		"key": "ctrl+`",
		"command": "-workbench.action.terminal.toggleTerminal",
		"when": "terminal.active"
	},
	{
		"key": "ctrl+t",
		"command": "-workbench.action.showAllSymbols"
	},
	{
		"key": "alt+w",
		"command": "-toggleSearchEditorWholeWord",
		"when": "inSearchEditor && searchInputBoxFocus"
	},
	{
		"key": "alt+w",
		"command": "-workbench.action.terminal.toggleFindWholeWord",
		"when": "terminalFindFocused && terminalProcessSupported || terminalFocus && terminalProcessSupported"
	},
	{
		"key": "alt+a",
		"command": "deleteLeft",
		"when": "textInputFocus"
	},
	{
		"key": "ctrl+alt+r",
		"command": "workbench.action.openRecent"
	},
	{
		"key": "ctrl+r",
		"command": "-workbench.action.openRecent"
	},
	{
		"key": "ctrl+alt+s",
		"command": "cursorDown",
		"when": "textInputFocus"
	},
	{
		"key": "ctrl+alt+w",
		"command": "cursorUp",
		"when": "textInputFocus"
	},
	{
		"key": "alt+w",
		"command": "editor.action.insertLineBefore",
		"when": "editorTextFocus && !editorReadonly"
	},
	{
		"key": "ctrl+shift+enter",
		"command": "-editor.action.insertLineBefore",
		"when": "editorTextFocus && !editorReadonly"
	},
	{
		"key": "alt+s",
		"command": "editor.action.insertLineAfter",
		"when": "editorTextFocus && !editorReadonly"
	},
	{
		"key": "ctrl+enter",
		"command": "-editor.action.insertLineAfter",
		"when": "editorTextFocus && !editorReadonly"
	},
	{
		"key": "ctrl+alt+q",
		"command": "editor.unfoldAll",
		"when": "editorTextFocus && foldingEnabled"
	},
	{
		"key": "ctrl+k ctrl+j",
		"command": "-editor.unfoldAll",
		"when": "editorTextFocus && foldingEnabled"
	}
]
